<scroll-view >


    <view class="container">
        <canvas canvas-id="pieCanvas" class="canvas" style="height:440rpx" bindtouchstart="touchHandler"></canvas>
    </view>
    <view class="scoView">


        <view class="space" bindtap="showCloud">
            <view class="cotentSpace">
                <view class="menuSpace">未打卡名单</view>
            </view>
            <text class="menuIcon2"></text>
        </view>


        <view class="scoView2" hidden="{{cloudFlag}}">


                <view class="line"></view>
                <view class="space2">
                    <view class="cotentSpace">
                        <view class="menuSpace">王一</view>
                    </view>
                </view>

                <view class="line"></view>
                <view class="space2">
                    <view class="cotentSpace">
                        <view class="menuSpace">张三</view>
                    </view>
                </view>

                <view class="line"></view>
                <view class="space2">
                    <view class="cotentSpace">
                        <view class="menuSpace">李四</view>
                    </view>
                </view>

        </view>

        <view class="space" bindtap="showPhoto">
            <view class="cotentSpace">
                <view class="menuSpace">已打卡情况</view>
            </view>
            <text class="menuIcon7"></text>
        </view>



        <view class="spacePhoto"  hidden="{{photoFlag}}" >
            <image src="{{imageUrl}}"  class="photo"></image>
            <view class="neme">张三</view>
        </view>






    </view>

    <text class="circle"></text>
</scroll-view>
